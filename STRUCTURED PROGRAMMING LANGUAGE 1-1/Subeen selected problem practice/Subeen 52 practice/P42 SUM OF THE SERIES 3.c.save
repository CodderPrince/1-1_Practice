#include<stdio.h>
int main()
{
    int t,n;
    scanf("%d", &t);
    for(int i=0;i<t; i++)
    {
        scanf("%d",&n);
        for(int k=n; k>=0; k--)
        {
            if(k==1)
            {
                printf("1\n");

            }

            else if(k==2)
            {
                printf("2 + ");
            }
            else
            {
                printf("2^%d + ",k);
            }
        }
        printf("\n");
    }


    return 0;
}
